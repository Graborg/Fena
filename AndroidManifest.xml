<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fena"
    android:versionCode="1"
    android:versionName="0.1" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="20" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@style/CustomActionBarTheme" >
        <activity
            android:name="com.fena.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.LogIn"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:parentActivityName="com.fena.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.LOGIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" >
            <meta-data android:name="android.app.searchable"
            android:resource="@drawable/searchable" />
            <intent-filter>
                <action android:name="android.intent.action.MAINFENA" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.MainActivityLogin"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" >
            <meta-data android:name="android.app.searchable"
            android:resource="@drawable/searchable" />
            <intent-filter>
                <action android:name="android.intent.action.MAINFENALOGIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.EditMyProfile"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:parentActivityName="com.fena.Profile" >
            <intent-filter>
                <action android:name="android.intent.action.REDPROFILE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.Profile"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:parentActivityName="com.fena.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.PROFILE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.ProjectView"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:parentActivityName="com.fena.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.PROJECT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.EditProject"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:parentActivityName="com.fena.ProjectView" >
            <intent-filter>
                <action android:name="android.intent.action.EDITPROJECT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.MyProfile"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:parentActivityName="com.fena.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MYPROFILE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.CreateMyProfile"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.CREATEMYPROFILE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.MyProjects"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:parentActivityName="com.fena.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MYPROJECTS" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.CreateProject"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.CREATEPROJECT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.MyProject"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:parentActivityName="com.fena.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MYPROJECT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fena.SendFeedback"
            android:screenOrientation="portrait"
            android:label="@string/app_name"
            android:parentActivityName="com.fena.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.SENDFEEDBACK" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>